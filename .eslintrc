{
  "plugins": [
    "angular"
  ],
  "rules": {
    "angular/ng_controller_name": [
      2,
      "/[A-Z].*Ctrl$/"
    ],
    "quotes": [
      2,
      "single"
    ],
    "no-use-before-define": true,
    "no-underscore-dangle": false,
    "new-cap": false
  },
  "globals": {
    "angular": true,
    "_": true,
    "ScrollsTypes": true
  }
}